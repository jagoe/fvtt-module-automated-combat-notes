{
  "$schema": "https://json.schemastore.org/foundryvtt-module-manifest.json",
  "id": "fvtt-module-automated-combat-notes",
  "title": "Automated Combat Notes",
  "description": "A Foundry VTT module that allows GMs and players to specify journal entries that get opened automatically once combat starts.",
  "authors": [
    {
      "name": "jagoe",
      "discord": "celtsfightnude",
      "email": "fvtt-acn@trtl.dev"
    }
  ],
  "version": "1.1.1",
  "packs": [],
  "scripts": [],
  "compatibility": {
    "minimum": "11",
    "verified": "11",
    "maximum": "11"
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "styles": [
    "style.css"
  ],
  "esmodules": [
    "scripts/module.js"
  ],
  "socket": true,
  "url": "https://github.com/jagoe/fvtt-module-automated-combat-notes",
  "readme": "https://github.com/jagoe/fvtt-module-automated-combat-notes/blob/main/README.md",
  "changelog": "https://github.com/jagoe/fvtt-module-automated-combat-notes/releases",
  "bugs": "https://github.com/jagoe/fvtt-module-automated-combat-notes/issues",
  "allowBugReporter": true
}
